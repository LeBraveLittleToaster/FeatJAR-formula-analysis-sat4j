<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
    <properties>
        <graphics key="autolayoutconstraints" value="false"/>
        <graphics key="legendautolayout" value="true"/>
        <graphics key="showconstraints" value="true"/>
        <graphics key="showshortnames" value="false"/>
        <graphics key="layout" value="horizontal"/>
        <graphics key="showcollapsedconstraints" value="true"/>
        <graphics key="legendhidden" value="false"/>
        <graphics key="layoutalgorithm" value="1"/>
    </properties>
    <struct>
        <and abstract="true" mandatory="true" name="WorkPlace">
            <graphics key="collapsed" value="false"/>
            <or abstract="true" mandatory="true" name="Desk">
                <and name="StandingDesk">
                    <feature name="Electric"/>
                    <feature name="CableCanal"/>
                </and>
                <and name="BasicDesk">
                    <feature name="MetalFeet"/>
                </and>
            </or>
            <and name="Ergonomics">
                <feature name="Treatmill"/>
                <feature name="AntifatiqueMatt"/>
                <feature name="HeadphoneHolder"/>
            </and>
        </and>
    </struct>
    <constraints>
        <rule>
            <imp>
                <var>CableCanal</var>
                <not>
                    <var>HeadphoneHolder</var>
                </not>
            </imp>
        </rule>
        <rule>
            <imp>
                <var>Treatmill</var>
                <not>
                    <var>MetalFeet</var>
                </not>
            </imp>
        </rule>
    </constraints>
</featureModel>
